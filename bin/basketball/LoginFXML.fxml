      <children>
        <Label text="Name" />
        <TextField fx:id="nameFld" alignment="CENTER" prefWidth="200.0" />
      </children>
    </HBox>
    <HBox id="HBox" alignment="CENTER" layoutX="56.0" layoutY="162.0" spacing="77.0">
      <children>
        <Label text="Password" />
        <PasswordField fx:id="passwordFld" alignment="CENTER" prefWidth="200.0" />
      </children>
    </HBox>
    <HBox id="HBox" alignment="CENTER" layoutX="142.0" layoutY="234.0" spacing="75.0">
      <children>
        <Button fx:id="okBtn" mnemonicParsing="false" onAction="#okBtnAction" text="OK" />
        <Button fx:id="exitBtn" mnemonicParsing="false" onAction="#exitBtnAction" text="EXIT" />
      </children>
    </HBox>
  </children>
